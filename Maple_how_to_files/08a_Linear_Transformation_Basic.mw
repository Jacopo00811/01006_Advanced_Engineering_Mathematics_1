dim(kerf) = number of free parameters = n - <Equation executable="false" style="2D Math" input-equation="" display="LUklbXJvd0c2Iy9JK21vZHVsZW5hbWVHNiJJLFR5cGVzZXR0aW5nR0koX3N5c2xpYkdGJzYlLUkjbWlHRiQ2JlEmJnJobztGJy8lJ2l0YWxpY0dRJmZhbHNlRicvJStleGVjdXRhYmxlR0YxLyUsbWF0aHZhcmlhbnRHUSdub3JtYWxGJ0YyRjQ=">LUklbXJvd0c2Iy9JK21vZHVsZW5hbWVHNiJJLFR5cGVzZXR0aW5nR0koX3N5c2xpYkdGJzYlLUkjbWlHRiQ2JlEmJnJobztGJy8lJ2l0YWxpY0dRJmZhbHNlRicvJStleGVjdXRhYmxlR0YxLyUsbWF0aHZhcmlhbnRHUSdub3JtYWxGJ0YyRjQ=</Equation>(<Font bold="true">F</Font>) = 3 - 2 =<Font bold="true"> </Font>1<Font bold="true"> </Font></Text-field>
